{
  "name": "components-vue",
  "version": "0.0.1",
  "description": "components for vuejs",
  "main": "index.coffee",
  "scripts": {
    "compile-gulp": "coffee -c gulpfile.coffee",
    "quick-start": "npm run compile-gulp && gulp clean && gulp build && gulp webserver",
